<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gtk_test">
	<schema id="@app-id@" path="/nl/emphisia/icon/">
	  <key name="folder-svg-path" type="s">
      <default>"/run/host/share/icons/Adwaita/scalable/places/folder.svg"</default>
      <summary>Folder SVG Path</summary>
    </key>
	  <key name="folder-cache-name" type="s">
      <default>'folder.svg'</default>
      <summary>name of folder icon in cache</summary>
    </key>
	  <key name="svg-render-size" type="i">
      <default>1024</default>
      <summary>SVG render resolution</summary>
    </key>
	  <key name="thumbnail-size" type="i">
      <default>256</default>
      <summary>Thumbnail render resolution</summary>
    </key>
    <key name="default-dnd-action" type="s">
      <default>'top'</default>
      <summary>Default drag-and-drop action</summary>
    </key>
    <key name="default-dnd-activated" type="b">
      <default>false</default>
      <summary>Whether or not the default drag-and-drop action is activated</summary>
    </key>
    <key name="selected-accent-color" type="s">
      <default>'None'</default>
      <summary>The accent color the user has selected in words, string is "None" if automatic change is selected</summary>
    </key>
    <key name="selected-accent-color-index" type="i">
      <default>0</default>
      <summary>The accent color the user has selected as index for row</summary>
    </key>
    <key name="manual-bottom-image-selection" type="b">
      <default>false</default>
      <summary>If true, the user has set a custom image for the bottom</summary>
    </key>
	</schema>
</schemalist>
